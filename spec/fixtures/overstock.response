		testDefaults: 'testDefaults',
		$addToCartForm: jQuery('#addToCartForm'),
		// zipUpdateLinkSelector: '#mainRightDeliveryEstimate .del-est-update-link',
		zipUpdateLinkSelector: '#mainRightDeliveryEstimate .del-est-update-button',
		zipUpdateInputSelector: '#mainRightDeliveryEstimate #delEstZipInput',
		shipRadioSelector: '#mainRightDeliveryEstimate .ship-delivery-opt-radio',
		loadingGifSelector: '#mainRightDeliveryEstimate .zip-loading img',
		$optionSelector: jQuery('[name=addid'+os.Simplicity.productId+']'),
		$qtySelector: jQuery('#addqty'+os.Simplicity.productId),
		currentSelectedShipOption: '',
		currentSelectedShipIndex: 0,
		currentProdQty: 1,
		monthNames: ['Jan.', 'Feb.', 'Mar.', 'Apr.', 'May', 'June', 'July', 'Aug.', 'Sept.', 'Oct.', 'Nov.', 'Dec.'],
		dayNames: ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'],
		productOptionSelected: false,
		pageLoadTrackingFired: false,
		fireTrackingFromZipChange: false,
		fireTrackingFromProdOptChange: false
	}

	return {
		init: function(settings) {
			var _this = this;

			jQuery.extend(defaults, settings);

			$('.mainRight-delivery').hide();
			if(!!defaults.data){
				//handle this differently: hide legacy treatment.

				if(!!defaults.$optionSelector.val()){
					defaults.productOptionSelected = true;
				}

				_this.setupHelpers();
				_this.buildMod(defaults.data, _this.afterModBuild);
				_this.bindings();

				//	fix for firefox: firefox remembers the state of select inputs.  if a product option was selected before a page refresh,
				//	the option would still be selected on page load and the shipping options and input would be enabled.
				//	check to see if there is a product option input (products with one option have a hidden input with no id). If there is,
				//	disable all shipping and delivery stuff.
				if($('select#addid'+os.Simplicity.productId).length>0){
					_this.disableShipOptions();
				}
			}
		},

		bindings: function() {
			var _this = this;

			defaults.$addToCartForm.on('click', defaults.zipUpdateLinkSelector, function(e) {
				e.preventDefault();
				_this.handleZipSubmit();
			});
			//there are two bindings for the zip input.  when hitting enter, the cart form tries to submit.  keypress event only that triggers that, not keyup.
			defaults.$addToCartForm.on('keypress', defaults.zipUpdateInputSelector, function(e) {
				if (e.keyCode == 13) {
					e.preventDefault();
					_this.handleZipSubmit();
				}
			});
			defaults.$addToCartForm.on('keyup', defaults.zipUpdateInputSelector, function(e) {
				_this.zipValidate(_this, $(this), e);
			});
			defaults.$addToCartForm.on('change', defaults.shipRadioSelector, function(e) {
				// console.log($(defaults.shipRadioSelector).length);
				_this.handleShipOptionChange(_this, $(this), e);
			});

			jQuery('#addCartWrap_productOptions #addid' + os.Simplicity.productId).on('change.deliveryEstimate', function(e) {
				_this.handleProdOptChange(_this, $(this), e);
			});

			jQuery('#addCartWrap_addCartMain #addqty' + os.Simplicity.productId).on('change.deliveryEstimate', function(e) {
				_this.handleProdQtyChange(_this, $(this), e);
			});

			defaults.$addToCartForm.on('focus', defaults.zipUpdateInputSelector, function(e) {
				// _this.hideDisabledShipLayer();
			});
			defaults.$addToCartForm.on('blur', defaults.zipUpdateInputSelector, function(e) {
				// _this.showDisabledShipLayer();
			});
			jQuery('#mainRight_deliveryEstimate').on('click', '.whatIsDelEst', function(e){
				e.preventDefault();
				if(!defaults.productOptionSelected){
					return false;
				}
				$(this).colorbox({
					href: '/82780/static.html',
					title: 'What is a "Delivery Estimate"?',
					width: 480,
					height: 220,
					onComplete: function(){
						//TRACKING
						var tid = "PP:EstShip:Lightbox:click";
						_this.fireShipTracking(tid);
					}
				})
			});
			$('#addToCartForm').on('submit', function(){
				if(defaults.data.shipOptions.length==0){
					var selectedShipOpt = defaults.data.shipOptions[0];
				} else {
					var selectedShipOpt = defaults.data.shipOptions[defaults.currentSelectedShipIndex];
				}
				_this.buildAndFireTrackingString(_this, selectedShipOpt, 'AddtoCart');
			});
		},

		setupHelpers: function() {
			Handlebars.registerHelper('estimateTimeByType', function(deliveryTimeEstimate, options) {

				function returnExactDate(deliveryTimeEstimate){
					//IE8 doesn't support ISO 8601 which is getting returned from the server and looks like this: "2014-06-20T00:00:00.000-06:00".
					//Normally, we could just do this: date = new Date(deliveryTimeEstimateDto.exactDate);
					//But we can't, so the date needs to be parsed right here:
					var ymdPart = deliveryTimeEstimate.exactDate.split('T')[0];
					var ymdPartsArray = ymdPart.split('-');

					var date = new Date(ymdPartsArray[0], ymdPartsArray[1]-1, ymdPartsArray[2]);//months are 0 indexed hence the '-1'
					//end IE8 ISO 8601 fix.

					var dayName = defaults.dayNames[date.getUTCDay()];
					var monthName = defaults.monthNames[date.getUTCMonth()];
					var dayNumber = date.getDate();


					return new Handlebars.SafeString("<span class='ship-delivery-opt-msg'><a class='whatIsDelEst'>Delivery Estimate</a> of <span>" + dayName + ", " + monthName + " " + dayNumber + "</span></span>");
				}
				if (deliveryTimeEstimate.type.toLowerCase() === "exact_date") {
					return returnExactDate(deliveryTimeEstimate);
				} else if(deliveryTimeEstimate.type.toLowerCase() === "holiday_message"){
					if(!!deliveryTimeEstimate.exactDate){
						return returnExactDate(deliveryTimeEstimate);
					} else {
						return new Handlebars.SafeString("<span class='ship-delivery-opt-msg'>"+deliveryTimeEstimate.message+"</span>");
					}
				} else if(deliveryTimeEstimate.type.toLowerCase() === "time_range"){
					return new Handlebars.SafeString("<span class='ship-delivery-opt-msg'>" + deliveryTimeEstimate.timeRange.shipTimeMin +
						"-" + deliveryTimeEstimate.timeRange.shipTimeMax + " " + deliveryTimeEstimate.timeRange.shipTimeUnit + "</span>");
				} else if(deliveryTimeEstimate.type.toLowerCase() === "error_message"){
					//console.log("deliveryTimeEstimate.type = ", deliveryTimeEstimate.type);
				}

			});
			Handlebars.registerHelper('shipOptionHasError', function(deliveryTimeEstimate, options) {
				if(deliveryTimeEstimate.type.toLowerCase() === "error_message" || deliveryTimeEstimate.type.toLowerCase() === "special_message"){
					return options.fn(this);
				} else {
					return options.inverse(this);
				}
			});

			Handlebars.registerHelper('shipNameAndChargeByType', function(deliveryTimeEstimate, options) {
				if(deliveryTimeEstimate.type.toLowerCase() === "holiday_message"){
					return options.fn(this);
				} else {
					return options.inverse(this);
				}
			});

			Handlebars.registerHelper('shipChargeFormat', function(charge, options) {
				charge = isNaN(charge) || charge === '' || charge === null ? 0.00 : charge;
				return parseFloat(charge).toFixed(2);
			});

			Handlebars.registerHelper('isMultiShipOpt', function(shipOpts, options) {
				if(shipOpts.length > 1){
					return options.fn(this);
				} else {
					return options.inverse(this);
				}
			});

		},

		doAjax: function(url, successCallback, errorCallback, _this) {
			$.ajax({
				url: url,
				type: "GET",
				dataType: 'json'
			}).success(function(res) {
				successCallback(res, _this);
			}).error(function(res) {
				errorCallback(res, _this);
			})
		},
		fireShipTracking: function(tid){
			s.linkTrackVars = "eVar4";
			s.eVar4 =  tid;
			s.tl(this,'o','PP Delivery Estimates');
		},
		disableShipOptions: function(){
			defaults.productOptionSelected = false;
			jQuery('#mainRightDeliveryEstimate').addClass('no-pro-option-selected');
			//disable radios
			$('.ship-delivery-opt-radio').attr('disabled', 'disabled');
			//disable zip input
			$(defaults.zipUpdateInputSelector).attr('disabled', 'disabled');
		},
		enableShipOptions: function(){
			defaults.productOptionSelected = true;
			jQuery('#mainRightDeliveryEstimate').removeClass('no-pro-option-selected');
			$('.ship-delivery-opt-radio').removeAttr('disabled');
			$(defaults.zipUpdateInputSelector).removeAttr('disabled');
			// console.log('en$: ', $(defaults.zipUpdateInputSelector));
		},

		buildAndFireTrackingString: function(_this, thisShipOpt, triggeredBy){
			if(typeof thisShipOpt === "undefined"){
				tid = "PP:EstShip:"+triggeredBy+":NoZip";
			} else {
				// var triggeredByString = "",
				// 	radioButtonString = ":RadioButton",
				var shortMsgString = ":"+thisShipOpt.shortMsg;

				// if(triggeredBy == "PGLoad"){
				// 	shortMsgString = "";
				// }

				// 	console.log(triggeredBy);
				// if(triggeredBy == "pageload"){
				// 	triggeredByString = ":PGLoad";
				// 	radioButtonString = "";
				// 	shortMsgString = "";
				// } else if(triggeredBy == "addtocart"){
				// 	radioButtonString = ":AddtoCart";
				// } else if(triggeredBy == "zipchange"){
				// 	triggeredByString = ":Zip";
				// 	radioButtonString = "";
				// } else if(triggeredBy == "prodchange"){
				// 	triggeredByString = ":OptionChange";
				// 	radioButtonString = "";
				// }

				function getTrackingChargeRange(charge){
					var string = '';
					if (!charge) { string = 'Free';} else
					if (charge < 9.99) { string = '$0.01_$9.99';} else
					if (charge < 19.99) { string = '$10.00_$19.99';} else
					if (charge < 29.99) { string = '$20.00_$29.99';} else
					if (charge < 39.99) { string = '$30.00_$39.99';} else
					if (charge < 49.99) { string = '$40.00_$49.99';} else
					if (charge < 59.99) { string = '$50.00_$59.99';} else
					if (charge < 69.99) { string = '$60.00_$69.99';} else
					if (charge < 79.99) { string = '$70.00_$79.99';} else
					if (charge < 89.99) { string = '$80.00_$89.99';} else
					if (charge < 99.99) { string = '$90.00_$99.99';} else
					if (charge < 199.99) { string = '$100.00_$199.99';} else
					if (charge < 299.99) { string = '$200.00_$299.99';} else
					if (charge >= 300.00) { string = '$300+';} else
					{ string = '$invalid';}
					return string;
				}
				function formatTrackingRange(timeRange){
					var string = '';
					string += timeRange.shipTimeMin;
					string += '_';
					string += timeRange.shipTimeMax;
					string += timeRange.shipTimeUnit.replace(/ /g,'');
					return string;
				}
				function formatTrackingExactDate(exactDate){
					//IE8 doesn't support ISO 8601 which is getting returned from the server and looks like this: "2014-06-20T00:00:00.000-06:00".
					//Normally, we could just do this: date = new Date(deliveryTimeEstimateDto.exactDate);
					//But we can't, so the date needs to be parsed right here:
					var ymdPart = exactDate.split('T')[0];
					var ymdPartsArray = ymdPart.split('-');

					var todayDate = new Date().getTime();
					var delDate = new Date(ymdPartsArray[0], ymdPartsArray[1]-1, ymdPartsArray[2]).getTime();//months are 0 indexed hence the '-1'

					var dayDiff = Math.ceil((delDate - todayDate)/(24*60*60*1000));

					return dayDiff += "Days";
				}
				if(!!thisShipOpt.deliveryTimeEstimate.timeRange){
					tid = "PP:EstShip:"+triggeredBy+shortMsgString+":DlvryRng:"+
					formatTrackingRange(thisShipOpt.deliveryTimeEstimate.timeRange)+":"+
					getTrackingChargeRange(thisShipOpt.charge)+":"+
					"Qty"+defaults.currentProdQty;
				} else if(!!thisShipOpt.deliveryTimeEstimate.exactDate){
					tid = "PP:EstShip:"+triggeredBy+shortMsgString+":DlvryEst:"+
					formatTrackingExactDate(thisShipOpt.deliveryTimeEstimate.exactDate)+":"+
					getTrackingChargeRange(thisShipOpt.charge)+":"+
					"Qty"+defaults.currentProdQty;
				} else if(thisShipOpt.deliveryTimeEstimate.type.toLowerCase() === 'holiday_message'){
					//holiday shipping can be range, or date, or neither.  instead, just send the message for the tracking.
					tid = "PP:EstShip:"+triggeredBy+shortMsgString+":DlvryEst:"+
					thisShipOpt.deliveryTimeEstimate.message.split(" ").join("")+":"+
					getTrackingChargeRange(thisShipOpt.charge)+":"+
					"Qty"+defaults.currentProdQty;
				} else if(thisShipOpt.deliveryTimeEstimate.type.toLowerCase() === 'error_message'){
					tid="PP:EstShip:"+triggeredBy+":Unshippable";
				}
			}
			_this.fireShipTracking(tid);
		},

		handleShipOptionChange: function(_this, $this, e){
			defaults.currentSelectedShipOption = $this.val();
			defaults.currentSelectedShipIndex = $this.data('index');

			//TRACKING
			var tid = '';
			var thisShipOpt = defaults.data.shipOptions[$this.data('index')];
			_this.buildAndFireTrackingString(_this, thisShipOpt, 'RadioButton');
			//TRACKING
		},


		//PRODUCT OPTION CHANGE
		handleProdOptChange: function(_this, $this, e){
			defaults.currentProdQty = 1;//THIS GETS RESET WHEN CHANGING PRODUCT OPTION.  defaults.$qtySelector.val();
			var newOption = jQuery($this).find(':selected').val(),
			currentZip = jQuery('#delEstZipHiddenInput').val(),
			url = '/api2/products/options/' + newOption + '/deliveryestimates?zipCode=' + currentZip + '&qty=' + defaults.currentProdQty;

			if(!newOption){
				_this.disableShipOptions();
				// console.log('no pro option');
			} else {
				_this.enableShipOptions();
				// console.log('yes pro option');
			}

			if(!!newOption && !!currentZip){
				//console.log('url: ', url);
				defaults.$addToCartForm.find(defaults.loadingGifSelector).removeClass('visibilityHidden');//SHOW LOADING
				_this.doAjax(url, _this.prodOptChangeSuccess, _this.prodOptChangeError, _this);
			} else {
				//console.log('handleProdOptChange: no option and/or zip: ', url);
			}
		},
		prodOptChangeSuccess: function(res, _this){
			defaults.data = res;
			defaults.$addToCartForm.find(defaults.loadingGifSelector).addClass('visibilityHidden');//HIDE LOADING

			defaults.fireTrackingFromProdOptChange = true;
			_this.buildMod(res, _this.afterModBuild);
		},
		prodOptChangeError: function(){
			defaults.$addToCartForm.find(defaults.loadingGifSelector).addClass('visibilityHidden');//HIDE LOADING
			//console.log("Ajax Error after handleProdOptChange");
		},


		//PRODUCT QUANTITY CHANGE
		handleProdQtyChange: function(_this, $this, e){
			defaults.currentProdQty = jQuery($this).find(':selected').val();
			var currentZip = jQuery('#delEstZipHiddenInput').val(),
			currentOption = defaults.$optionSelector.val();
			url = '/api2/products/options/' + currentOption + '/deliveryestimates?zipCode=' + currentZip + '&qty=' + defaults.currentProdQty;

			if(!!currentOption && !!currentZip){
				//console.log('url: ', url);
				defaults.$addToCartForm.find(defaults.loadingGifSelector).removeClass('visibilityHidden');//SHOW LOADING
				_this.doAjax(url, _this.prodQtyChangeSuccess, _this.prodQtyChangeError, _this);
			} else {
				//console.log('handleProdQtyChange: no option and/or zip: ', url);
			}
		},
		prodQtyChangeSuccess: function(res, _this){
			defaults.data = res;
			defaults.$addToCartForm.find(defaults.loadingGifSelector).addClass('visibilityHidden');//HIDE LOADING
			_this.buildMod(res, _this.afterModBuild);
		},
		prodQtyChangeError: function(){
			defaults.$addToCartForm.find(defaults.loadingGifSelector).addClass('visibilityHidden');//HIDE LOADING
			//console.log("Ajax Error after handleProdQtyChange");
		},

		zipValidate: function(_this, $zipInput, event) {
			var val = $zipInput.val();
			var intRegex = /^\d+$/; //(BOOL) IS INTEGER
			if (!intRegex.test(val)) {
				$zipInput.val($zipInput.val().replace(/[^\d]/g, "")); //IF VALUE IS NOT AN INT (ZIP), REMOVE NON-INT CHARACTERS.
				_this.enforceMaxlengthOnTextInput($zipInput); //ONLY ALLOW MAXLENGTH OF CHARACTERS (5)
			}
		},
		enforceMaxlengthOnTextInput: function($textinput) {
			var val = $textinput.val(),
				maxlength = $textinput.attr('maxlength');
			if (val.length > maxlength) {
				val = val.slice(0, maxlength);
				$textinput.val(val);
				var range = $textinput.createTextRange();
				range.collapse(false);
				range.select();
			}
		},
		handleZipSubmit: function() {
			var _this = this,
				$newZipInput = jQuery(defaults.zipUpdateInputSelector),
				newZipVal = $newZipInput.val(),
				isValidZip = /(^\d{5}$)/.test(newZipVal),
				$zipError = jQuery('#mainRightDeliveryEstimate .zip-error span'),
				existingZipVal = jQuery('#delEstZipHiddenInput').val(),
				changesMade = false;

			function sendUpdates(){

				var optionId=defaults.$optionSelector.val()||defaults.data.productOptionId;

				var url = '/api2/products/options/' + optionId + '/deliveryestimates?zipCode=' + newZipVal + '&qty=' + defaults.currentProdQty;
				_this.doAjax(url, _this.handleZipSuccess, _this.handleZipError, _this);

			}

			//if there is no product option selected, show error message
			if(!defaults.productOptionSelected){
				return false;
			}

			defaults.$addToCartForm.find(defaults.loadingGifSelector).removeClass('visibilityHidden');//SHOW LOADING

			changesMade = existingZipVal != newZipVal;

			if (isValidZip) { //CHECK IF VALID
				$zipError.hide();//HIDE ERRORS
				sendUpdates();
			} else {
				setTimeout(function() {
					var data = {
						status: 400//simulate 400 from server (invalid zip);
					}
					_this.handleZipError(data, _this);
				}, 100);
				return;
			}
		},

		handleZipSuccess: function(data, _this) {
			defaults.$addToCartForm.find(defaults.loadingGifSelector).addClass('visibilityHidden');//HIDE LOADING
			defaults.data = data;





			//TRACKING
			// var firstShipOpt = defaults.data.shipOptions[0];
			// switch(firstShipOpt.deliveryTimeEstimate.type.toLowerCase()) {
			// 	case "exact_date":
			// 		tid = "PP:EstShip:Zip:DlvryEst";
			// 		break;
			// 	case "holiday_message":
			// 		if(!!firstShipOpt.deliveryTimeEstimate.exactDate){
			// 			tid = "PP:EstShip:Zip:DlvryEst:Holiday";
			// 		} else {
			// 			tid = "PP:EstShip:Zip:DlvryRng:Holiday";
			// 		}
			// 		break;
			// 	case "time_range":
			// 		tid = "PP:EstShip:Zip:DlvryRng";
			// 		break;
			// 	case "error_message":
			// 		tid = "PP:EstShip:Zip:Unshippable";
			// 		break;
			// 	case "special_message":
			// 		tid = "PP:EstShip:Zip:SPECIAL";
			// 		break;
			// 	default:
			// 		tid = "PP:EstShip:Zip:Unhandled";
			// }
			// _this.fireShipTracking(tid);
			//TRACKING

			defaults.fireTrackingFromZipChange = true;
			_this.buildMod(data, _this.afterModBuild);
		},
		handleZipError: function(data, _this) {
			// console.log('thisdata', data);
			var $zipError = jQuery('#mainRightDeliveryEstimate .zip-error span'),
			$shipOpts = jQuery('#mainRightDeliveryEstimate .del-est-ship-opts'),
			status = data.status,
			errorZipMsg = "",
			zipInputVal = $(defaults.zipUpdateInputSelector).val();

			if(status == 400){
				errorZipMsg = "Invalid zip code";
			} else if(status == 500){
				errorZipMsg = "This item cannot be shipped to '"+zipInputVal+"'";
			} else {
				errorZipMsg = "There was an error getting shipping info";
				//TRACKING
				var tid = "PP:EstShip:Zip:NoServerResponse/Timeout";
				_this.fireShipTracking(tid);
			}

			defaults.$addToCartForm.find(defaults.loadingGifSelector).addClass('visibilityHidden');//HIDE LOADING

			$zipError.text(errorZipMsg).show();
			$shipOpts.find('ul').remove();

			defaults.data.shipOptions = [];

			//TRACKING
			var tid = "PP:EstShip:Zip:Invalid";
			_this.fireShipTracking(tid);
		},

		buildMod: function(data, callback) {
			var _this = this;
			var delEstTemplate = Handlebars.templates['del_est_mod'];
			if (!!jQuery('#mainRightDeliveryEstimate').length) {
				jQuery('#mainRightDeliveryEstimate').remove();
			}
			//todo:
			// $('#mainRight_wishListSaveLater').after(delEstTemplate(data));
			$('#mainRight_deliveryEstimate').html(delEstTemplate(data));

			//bind tooltip
			os.ToolTips('#mainRightDeliveryEstimate .tooltip', {
				offset: {
					x: 245,
					y: -75
				},
				hideTimer: 25000,
				toolTipContent: function() {
					return jQuery('<div class="info-tooltip del-est-tooltip"><div>What is a "Delivery Estimate"?</div><br>'+
						'<div>This is an approximate delivery date and is not a guarantee. '+
						'Delivery estimates are available only for select items to be delivered '+
						'in the contiguous United States, excluding APO/FPO addresses and PO boxes. '+
						'Your final shipping cost will be available at checkout. </div> '+
						'<div>These delivery estimates apply to orders placed today. Continue to '+
						'checkout to select a shipping option and update your estimated delivery date.</div></div>');
				}
			});


			//TRACKING
			if(!defaults.pageLoadTrackingFired){
				defaults.pageLoadTrackingFired = true;
				_this.buildAndFireTrackingString(_this, data.shipOptions[0], 'PGLoad');
			}

			if(defaults.fireTrackingFromZipChange){
				defaults.fireTrackingFromZipChange = false;
				_this.buildAndFireTrackingString(_this, data.shipOptions[defaults.currentSelectedShipIndex], 'Zip');
			}

			if(defaults.fireTrackingFromProdOptChange){
				defaults.fireTrackingFromProdOptChange = false;
				_this.buildAndFireTrackingString(_this, data.shipOptions[defaults.currentSelectedShipIndex], 'OptionChange');
			}
			//TRACKING


			if (typeof callback === "function") {
				callback(_this);
			}
		},
		afterModBuild: function(_this) {
			var numShipOptions = defaults.data.shipOptions.length;
			if (numShipOptions > 1) {
				var $selectThisRadio;
				if($('#shipOptId'+defaults.currentSelectedShipOption).length>0){
					$selectThisRadio = $('#shipOptId'+defaults.currentSelectedShipOption);
				} else {
					$selectThisRadio = $('.ship-delivery-opt-radio')[0];
				}
				$($selectThisRadio).attr('checked', 'checked');
			}

			// disable ship options if no value is selected.
			if(!defaults.productOptionSelected){
				_this.disableShipOptions();
			}
			//  else {
			// 	defaults.productOptionSelected = true;
			// }

		}
	}
})();
</script>


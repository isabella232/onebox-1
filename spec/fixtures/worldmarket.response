function cpmw_opti_popGlobalCart(s){


	var showCloseLink = !!s;
	console.log('cpmw_opti_popGlobalCart - showCloseLink='+s)

	//redefine showGlobalCart
	dontshowGlobalCartAfterAddToCart_showGlobalBasket = MarketLive.GlobalCart.showGlobalBasket;

	MarketLive.GlobalCart.showGlobalBasket = function () {
		if ( MarketLive.GlobalCart.expandOnLoadAction ) { // do popup

			var popCart = jQuery("div.popDownLayer.globalCartLayer")
				.clone()
				.css({position:"relative", width: 360}).removeClass("popDownLayer")
				.show();
			// create popup cart wrapper
			jQuery('<div/>', {id: "popupGlobalCart"}).append(popCart).appendTo('body');

			if (showCloseLink) {
				jQuery('#popupGlobalCart div.globalCartLayer .globalCartContinueShopping').css({padding: "15px 0 0 10px"});
				jQuery('#popupGlobalCart div.globalCartLayer .globalCartContinueShopping a').css({color: "#1B9990"}).unbind("click").click(function(){cartPop.unpop();});
			} else {
				jQuery('#popupGlobalCart div.globalCartLayer .globalCartContinueShopping').remove()
			}

			// add header
			jQuery('<div>Item has been added to your cart</div>')
				.css({fontFamily:"SofiaProLight", fontSize: 20, color:"#666", padding: "15px 10px 10px 10px", borderBottom: "1px solid  #eee"})
				.prependTo("#popupGlobalCart div.globalCartLayer");

			// set up popupCart wrapper to pop
			cartPop = jQuery("#popupGlobalCart").simplePopup()

			// pop it
			cartPop.makemodal().pop()
			setTimeout(cartPop.unpop, 6000);

		} else { // just run it
			dontshowGlobalCartAfterAddToCart_showGlobalBasket()
		}

	}
}


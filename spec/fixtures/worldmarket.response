<script type="text/javascript">
var altThumbWidth=(62+2+2+10); // used below
	function checkAlts() {
	/*
		function looks at the image tags in the detailViewContainer, and checks the src URL
		if the URL is bad (no image loads) it hides this image
	*/
		visiblealts = 0; // global
		jQuery("div#detailViewContainer a img").each(
			function (i,o) {
				jQuery(o).hide();     // start by hiding this image - it effectively collapses
				var ti=new Image();   // new Img object
				ti.loaded = false;		// set it's custom prop "loaded" to false
				ti.onload = function() {  // set onload method to
						jQuery(o).show();  // display the image (since it loaded)
						visiblealts++      // increment global var of visible alts
					};
				ti.src=o.src; // now try to load the image
			}
		);
	}

	jQuery(document).ready(function(){
		checkAlts(); // run the check and collapse bad images

		// kill the mouseover eventhandler for swatches - set up in HTML source
		jQuery("#detailSwatchContainer a img").each(function(i,o){o.onmouseover = function () {};})

		// add a click handler that checks the images and scrolls the altThumbs back to beginning, and calls the 1st image's click() method
		jQuery("#detailSwatchContainer a img").click( function() {
			checkAlts();
			jQuery("div#detailViewContainer").animate({left:"0px"});
			//jQuery("#detailViewContainer a img")[0].click();
		})

		jQuery("div#detailViewContainer a").each(
			function (i,o) {
				jQuery(o).mouseover(function () { // on mouseover check for view state and scroll as needed
					if (visiblealts<7)return     // have 5 images or fewer - do nothing
					if (jQuery("div#detailViewContainer").is(":animated")) return; // container is scrolling - do Nothing
					/* readout some positioning values */
					var os=jQuery("div#detailViewContainer").position().left; //left position of the scroller
					var ii=jQuery(o).index(); // index of this image (thank you jQuery!)
					if (os<0 && ii==-os/altThumbWidth){      // we've moved to left, so now scroll back
						jQuery("div#detailViewContainer").animate({left:"0px"})
						return false;
					}
					if (os==0 && ii>3) {     // we've not moved, so scroll to right
						jQuery("div#detailViewContainer").animate({left:-(altThumbWidth*(visiblealts-5))+"px"})
						return false;
					}
				});
			}
		);
		jQuery(".pqve_container_detail #detailViewContainer a").each(
			function (i,o) {
				jQuery(o).mouseover(function () { // on mouseover check for view state and scroll as needed
					if (visiblealts<6)return     // have 5 images or fewer - do nothing
					if (jQuery("div#detailViewContainer").is(":animated")) return; // container is scrolling - do Nothing
